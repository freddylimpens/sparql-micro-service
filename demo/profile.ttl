# --
# Service URL: http://localhost:8080/tutorial/demo?param=Delphinus+delphis
# --

st:demo a st:Server ;
    st:service "demo" ;

    st:param [
        st:export true ;
        st:hyperlink true ;
    ] ;

    # Select how to get the data (query vs. static file)
    st:workflow st:demoWorkflow ;
#    st:content st:democontent ;
    .

# Load data from a query
st:demoWorkflow a sw:Workflow ; 
    sw:body (
        [ a sw:Query ;          sw:uri <query/query.rq> ]
        [ a sw:Transformation ; sw:uri <sttl/> ]
    ).

# loading data from a file
st:democontent a sw:Workflow ;
    sw:body (
        [ a sw:Load ; sw:path <data/constr.ttl>  ; sw:name st:data1 ]    
    ).
